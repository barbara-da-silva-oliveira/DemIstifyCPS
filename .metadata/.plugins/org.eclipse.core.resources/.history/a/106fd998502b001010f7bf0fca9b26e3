package fr.inria.kairos.influence.validation;

import java.util.HashMap;

import org.eclipse.xtext.validation.Check;

import fr.inria.kairos.influence.metamodel.influenceMetamodel.Influence;



/**
 * This class contains custom validation rules. 
 *
 * See https://www.eclipse.org/Xtext/documentation/303_runtime_concepts.html#validation
 */
public class InfluenceDSLValidator extends AbstractInfluenceDSLValidator {
	HashMap<String, Integer> impactSource = new HashMap<String, Integer>();
	

    @Check
    public void checkFileModification(Influence model) {
        long modificationTimestamp = model.eResource().getModificationTimestamp();
 
        if (modificationTimestamp > 0) {
            error("File has been modified", model, MyPackage.Literals.MY_MODEL__NAME);
        }
    }

	
}
